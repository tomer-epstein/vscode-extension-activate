// A launch configuration that compiles the extension and then opens it inside a new window
// Use IntelliSense to learn about possible attributes.
// Hover to view descriptions of existing attributes.
// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-manager",
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-contrib1",
        "--extensionDevelopmentPath=${workspaceFolder}/vscode-activation-event",
    ],
      "outFiles": [
        "${workspaceFolder/vscode-vscode-manager/out/**/*.js",
        "${workspaceFolder/vscode-contrib1/out/**/*.js",
        "${workspaceFolder/vscode-activation-event/out/**/*.js"
      ]
    }
  ]
}